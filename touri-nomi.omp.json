{
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        // {
        //   "type": "text",
        //   "style": "plain",
        //   "foreground": "#fff",
        //   "properties": {
        //     "template": "\ufc70"
        //   }
        // },
        {
          "type": "session",
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#486FA5",
          "properties": {
            "template": "{{ .UserName }}@{{if (.SSHSession)}}{{.SSHSession}}{{else}}{{.ComputerName}}{{end}}"
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#000000",
          "background": "#FDFDFE",
          "properties": {
            "prefix": " \uE5FF ",
            "style": "folder",
            "enable_hyperlink": true
          }
        }, 
        {
          "type": "git",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "foreground": "#ffffff",
          "background": "#273373",
          
          "properties": {
            "fetch_upstream_icon": true,
            "fetch_status": true,
            "github_icon": "\ue709",
            "template": "{{.UpstreamIcon}} [{{.Upstream}}]({{.UpstreamURL}})"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      // "vertical_offset": 1,
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#fff",
          "properties": {
            "template": "\ufb0c"
          }
        }
      ]
    }
  ]
}